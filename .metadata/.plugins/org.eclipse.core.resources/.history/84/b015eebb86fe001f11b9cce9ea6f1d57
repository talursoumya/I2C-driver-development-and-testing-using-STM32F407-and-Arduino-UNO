#ifndef INC_STM32F407XX_USART_H_
#define INC_STM32F407XX_USART_H_

#include"common.h"

typedef struct
{
	uint32_t USART_SR;
	uint32_t USART_DR;
	uint32_t USART_BRR;
	uint32_t USART_CR1;
	uint32_t USART_CR2;
	uint32_t USART_CR3;
	uint32_t USART_GPTR;

}USART_RegDef_t;

#endif /* INC_STM32F407XX_USART_H_ */
